{
    "name": "ytake/laravel-voltdb",
    "description": "VoltDB providers for Laravel",
    "keywords": [
        "laravel",
        "auth",
        "cache",
        "session",
        "voltdb",
        "database",
        "newsql"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Yuuki Takezawa",
            "email": "yuuki.takezawa@comnect.jp.net"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "ext-curl": "*",
        "ytake/voltdb-client-wrapper": "0.*",
        "illuminate/support": "4.2.*",
        "illuminate/config": "4.2.*",
        "illuminate/auth": "4.2.*",
        "illuminate/database": "4.2.*",
        "illuminate/hashing": "4.2.*",
        "illuminate/filesystem": "4.2.*",
        "illuminate/cache": "4.2.*",
        "illuminate/console": "4.2.*",
        "illuminate/session": "4.2.*",
        "illuminate/events": "4.2.*"
    },
    "require-dev": {
        "symfony/framework-bundle": "2.*",
        "symfony/console": "2.*",
        "phpunit/phpunit": "4.*",
        "mockery/mockery": "0.*",
        "satooshi/php-coveralls": "dev-master"
    },
    "suggest": {
        "ext-voltdb": "for AuthDriver, SessionDriver, CacheDriver. use extension https://github.com/VoltDB/voltdb-client-php/tree/native"
    },
    "autoload": {
        "psr-4": {
            "Ytake\\LaravelVoltDB\\": "src/VoltDB"
        }
    },
    "minimum-stability": "stable"
}
