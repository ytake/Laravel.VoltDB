{
    "name": "ytake/laravel-voltdb",
    "description": "VoltDB providers for Laravel",
    "keywords": ["laravel","auth","voltdb","newsql"],
    "license": "MIT",
    "authors": [
        {
            "name": "Yuuki Takezawa",
            "email": "yuuki.takezawa@comnect.jp.net"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "ext-voltdb": "*",
        "ext-curl": "*",
        "ytake/voltdb-client-wrapper": "0.*",
        "illuminate/support": "4.2.*",
        "illuminate/config": "4.2.*",
        "illuminate/auth": "4.2.*",
        "illuminate/database": "4.2.*",
        "illuminate/hashing": "4.2.*",
        "illuminate/filesystem": "4.2.*",
        "illuminate/cache": "4.2.*",
        "illuminate/console": "4.2.*"
    },
    "require-dev": {
        "phpunit/phpunit": "4.*",
        "mockery/mockery": "0.*"
    },
    "autoload": {
        "psr-4": {
            "Ytake\\LaravelVoltDB\\": "src/VoltDB"
        }
    },
    "minimum-stability": "stable"
}
