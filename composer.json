{
    "name": "ytake/laravel-voltdb",
    "description": "VoltDB providers for Laravel",
    "keywords": [
        "laravel",
        "auth",
        "cache",
        "session",
        "voltdb",
        "database",
        "newsql"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Yuuki Takezawa",
            "email": "yuuki.takezawa@comnect.jp.net"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "ext-curl": "*",
        "ytake/voltdb-client-wrapper": "0.*",
        "illuminate/support": "5.0.*@dev",
        "illuminate/config": "5.0.*@dev",
        "illuminate/auth": "5.0.*@dev",
        "illuminate/http": "5.0.*@dev",
        "illuminate/database": "5.0.*@dev",
        "illuminate/hashing": "5.0.*@dev",
        "illuminate/filesystem": "5.0.*@dev",
        "illuminate/cache": "5.0.*@dev",
        "illuminate/console": "5.0.*@dev",
        "illuminate/container": "5.0.*@dev",
        "illuminate/contracts": "5.0.*@dev",
        "illuminate/session": "5.0.*@dev",
        "illuminate/events": "5.0.*@dev",
        "illuminate/encryption": "5.0.*@dev"
    },
    "require-dev": {
        "symfony/framework-bundle": "2.*",
        "symfony/console": "2.*",
        "phpunit/phpunit": "4.*",
        "mockery/mockery": "0.*",
        "satooshi/php-coveralls": "dev-master"
    },
    "suggest": {
        "ext-voltdb": "for AuthDriver, SessionDriver, CacheDriver. use extension https://github.com/VoltDB/voltdb-client-php/tree/native"
    },
    "autoload": {
        "psr-4": {
            "Ytake\\LaravelVoltDB\\": "src/VoltDB"
        }
    },
    "autoload-dev": {
        "files": [
            "tests/TestCase.php"
        ]
    },
    "minimum-stability": "stable"
}
